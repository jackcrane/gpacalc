{
	"manifest_version":2,
	"name":"Myschoolapp GPA Calculator",
	"version":"1.2",
	"description": "Simple front-end GPA calculator for Myschoolapp",
	"icons":{"16":"favicon.png","48":"favicon.png","128":"favicon.png"},
	"content_scripts": [{
		"matches": [
			"https://*.myschoolapp.com/*"
		],
		"js":["script.js"]
	}],
	"permissions": ["activeTab"]
}